TextureCube CubeMapTexture : register(t0);
SamplerState ClampSampleType : register(s0);

struct PixelInputType {
    float4 position : SV_POSITION;
    float3 uvw : TEXCOORD0;
};

float4 Frag(PixelInputType i) : SV_TARGET {
    float4 color = CubeMapTexture.Sample(ClampSampleType, i.uvw);
    //float4 color = CubeMapTexture.SampleLevel(ClampSampleType , i.uvw, 1.2);
    return color;
}